{
  "name": "mizner/candidates",
  "description": "Mizner Candidate Template",
  "authors": [
    {
      "name": "Michael Mizner",
      "email": "mike@mizner.io"
    }
  ],
  "extra": {
    "installer-paths": {
      "wp-content/themes/{$name}/": [
        "type:wordpress-theme"
      ],
      "wp-content/plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "wp-content/mu-plugins/{$name}/": [
        "type:wordpress-muplugin"
      ]
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "git",
      "url": "git@bitbucket.org:pyxlinc/wordpress-starter-theme.git"
    },
    {
      "type": "composer",
      "url": "https://composer.deliciousbrains.com/3998821A01BD70FAD9531E70B4F5EE17"
    },
    {
      "type": "package",
      "package": {
        "name": "advanced-custom-fields/advanced-custom-fields-pro",
        "version": "5.6.9",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://connect.advancedcustomfields.com/index.php?p=pro&a=download&k=b3JkZXJfaWQ9MzgzNjF8dHlwZT1kZXZlbG9wZXJ8ZGF0ZT0yMDE0LTA4LTI3IDE4OjQ1OjU3"
        }
      }
    }
  ],
  "require-dev": {
    "vlucas/phpdotenv": "^2.4"
  },
  "require": {
    "php": "^7.0",
    "pyxlinc/wordpress-starter-theme": "dev-master",
    "wpackagist-plugin/wordpress-seo": "^7.0",
    "wpackagist-plugin/regenerate-thumbnails": "^3.0",
    "advanced-custom-fields/advanced-custom-fields-pro": "*",
    "wpackagist-plugin/simple-page-ordering": "^2.3",
    "deliciousbrains-plugin/wp-offload-s3": "^1.5",
    "deliciousbrains-plugin/wp-migrate-db-pro-cli": "^1.3",
    "deliciousbrains-plugin/wp-migrate-db-pro": "^1.8",
    "wpackagist-plugin/post-duplicator": "^2.20"
  },
  "scripts": {
    "post-install-cmd": [
      "wp core download --path=core --skip-content --force",
      "bash setup.sh"
    ],
    "post-update-cmd": [
    ]
  }
}
